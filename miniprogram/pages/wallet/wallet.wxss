/* miniprogram/pages/address/address.wxss */

@import "../../../main.wxss";
.adress-list{
  border-bottom: 1px solid rgb(246, 246, 246);
  margin: 0 auto;
  background-color: white;
}
.change{
  margin-left: 10px;
  margin-top: 30px;
  text-align: right;
  font-size: 15px;
}

.name{
  font-size: 18px;
  color: black;
  font-weight: bold;
}

.address{
  font-size: 14px;
}

.main{
  display: flex;
  flex-direction: column;
  height: 100vh;
  justify-content: center;
}

.button-list{
  margin-top: auto;
}

.button{
  margin: 0 auto;
  color: rgb(17, 141, 231);
}